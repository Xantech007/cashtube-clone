<?php
$file = '../users/videos/' . $_GET['f'];

if (file_exists($file) && str_ends_with($file, '.mp4')) {
    header('Content-Type: video/mp4');
    header('Accept-Ranges: bytes');
    header('Content-Length: ' . filesize($file));
    readfile($file);
    exit;
}
http_response_code(404);
echo "File not found";
?>
